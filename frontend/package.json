{
  "name": "decidim-monitor",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng build",
    "build:prod": "ng build --prod --aot --progress=false",
    "watch": "ng build --watch --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "download_schema": "apollo-codegen introspect-schema http://localhost:4000/api --output src/schema.json",
    "pregenerate_types": "npm run download_schema",
    "generate_types": "apollo-codegen generate **/*.graphql --schema src/schema.json --target typescript --output src/app/graphql-types.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.9",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "6.0.9",
    "@angular/compiler": "6.0.9",
    "@angular/core": "6.0.9",
    "@angular/forms": "6.0.9",
    "@angular/http": "6.0.9",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "6.0.9",
    "@angular/platform-browser-dynamic": "6.0.9",
    "@angular/router": "6.0.9",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "apollo-angular": "^1.1.2",
    "apollo-angular-cache-ngrx": "^1.0.0-beta.0",
    "apollo-angular-link-http": "^1.1.1",
    "apollo-cache-inmemory": "^1.2.6",
    "apollo-client": "^2.3.7",
    "apollo-codegen": "^0.20.2",
    "core-js": "^2.5.7",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "ngrx-store-freeze": "^0.2.4",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "semver": "^5.5.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "6.0.9",
    "@ngrx/schematics": "^6.0.1",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.5.3",
    "codelyzer": "^4.4.2",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
